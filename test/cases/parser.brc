func main(args : str[]) : int
{
    var x : int = 52
    var y : int = 67

    if(x > y)
    {
        return x + y
    }
    else if(x < y)
    {
        return x - y
    }
    else
    {
        return x * y
    }

    while(x > 0)
    {
        x--
    }

    for(var i : int = 0; i < 10; i++)
    {
        x++
    }

    return 0
}